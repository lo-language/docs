/*
 * usage: main.lo <side length>
 */

Square  <Square>


main is <-> (args, io) {

	squareSide = args[0];

    square = Square:create(squareSide);

    io.out.write("side length is `square.getSide()`\n");

    area = square.getArea();

    io.out.write(square.toString());
    io.out.write("\n`area`\n");
};

